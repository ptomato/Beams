      <!-- column-name height -->
      <column type="guint"/>
    </columns>
  </object>
  <object class="GtkListStore" id="rotations">
    <columns>
      <!-- column-name display_name -->
      <column type="gchararray"/>
      <!-- column-name rotations -->
      <column type="guint"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">0°</col>
        <col id="1">0</col>
      </row>
      <row>
